{
   "name": "fetch-json",
   "description": "A wrapper around Fetch just for JSON (written in TypeScript)",
   "version": "2.6.4",
   "license": "MIT",
   "type": "module",
   "module": "dist/fetch-json.js",
   "jsdelivr": "dist/fetch-json.min.js",
   "main": "dist/fetch-json.umd.cjs",
   "types": "dist/fetch-json.d.ts",
   "files": [
      "dist"
   ],
   "exports": {
      ".": {
         "import": "./dist/fetch-json.js",
         "require": "./dist/fetch-json.umd.cjs"
      },
      "./": "./dist/"
   },
   "repository": "github:center-key/fetch-json",
   "homepage": "https://fetch-json.js.org",
   "bugs": "https://github.com/center-key/fetch-json/issues",
   "docs": "https://github.com/center-key/fetch-json#readme",
   "keywords": [
      "browser",
      "fetch",
      "fetch-json",
      "get",
      "http",
      "jamstack",
      "javascript",
      "json",
      "node-fetch",
      "post",
      "rest",
      "typescript"
   ],
   "jshintConfig": {
      "esversion": 9,
      "strict": "implied",
      "eqeqeq": true,
      "nonbsp": false,
      "undef": true,
      "unused": true,
      "varstmt": true,
      "browser": true,
      "mocha": true,
      "node": true,
      "globals": {
         "fetch": true
      }
   },
   "eslintConfig": {
      "ignorePatterns": [
         "build",
         "dist",
         "node_modules"
      ],
      "root": true,
      "parser": "@typescript-eslint/parser",
      "plugins": [
         "@typescript-eslint"
      ],
      "extends": [
         "eslint:recommended",
         "plugin:@typescript-eslint/recommended"
      ],
      "rules": {
         "@typescript-eslint/no-non-null-assertion": "off"
      }
   },
   "scripts": {
      "step:01": "rimraf build dist **/.DS_Store",
      "step:02": "jshint . --exclude node_modules,build,dist",
      "step:03": "eslint --max-warnings 0 . --ext .ts",
      "step:04": "tsc",
      "step:05": "tsc --module UMD --outDir build/umd",
      "step:06": "cpy build/umd/fetch-json.js build --rename=fetch-json.umd.cjs --flat=true",
      "step:07": "gulp minify-js",
      "step:08": "add-dist-header build dist",
      "step:09": "w3c-html-validator",
      "pretest": "npm-run-all step:*",
      "test": "mocha spec/*.spec.js --timeout 5000",
      "examples": "node docs/js/examples.js && tsc && node build/docs/ts/examples.js"
   },
   "devDependencies": {
      "@babel/core": "~7.17",
      "@babel/preset-env": "~7.16",
      "@fortawesome/fontawesome-free": "~6.0",
      "@types/node": "~17.0",
      "@typescript-eslint/eslint-plugin": "~5.15",
      "@typescript-eslint/parser": "~5.15",
      "add-dist-header": "~0.1",
      "assert-deep-strict-equal": "~1.0",
      "babel-minify": "~0.5",
      "canvas": "~2.9",
      "cpy-cli": "~4.1",
      "dna.js": "~1.9",
      "eslint": "~8.11",
      "gulp": "~4.0",
      "gulp-append-prepend": "~1.0",
      "gulp-babel": "~8.0",
      "gulp-rename": "~2.0",
      "gulp-replace": "~1.1",
      "gulp-size": "~4.0",
      "jquery": "~3.6",
      "jsdom": "~19.0",
      "jshint": "~2.13",
      "mocha": "~9.2",
      "node-fetch": "~3.2",
      "npm-run-all2": "~5.0",
      "promise-polyfill": "~8.2",
      "puppeteer": "~13.5",
      "puppeteer-browser-ready": "~0.4",
      "typescript": "~4.6",
      "w3c-html-validator": "~1.0",
      "web-ignition": "~1.5",
      "whatwg-fetch": "~3.6"
   }
}
