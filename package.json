{
   "name": "fetch-json",
   "description": "A wrapper around Fetch just for JSON",
   "version": "2.1.0",
   "license": "MIT",
   "main": "fetch-json.js",
   "repository": "github:center-key/fetch-json",
   "homepage": "https://github.com/center-key/fetch-json",
   "bugs": "https://github.com/center-key/fetch-json/issues",
   "docs": "https://github.com/center-key/fetch-json/blob/master/README.md",
   "keywords": [
      "browser",
      "fetch",
      "node-fetch",
      "get",
      "http",
      "javascript",
      "json",
      "post",
      "rest"
   ],
   "contributors": [
      "KahWee Teng (https://twitter.com/kahwee)",
      "Dem Pilafian (https://twitter.com/dempilafian)"
   ],
   "eslintConfig": {
      "extends": "eslint:recommended",
      "env": {
         "es6": true,
         "browser": true,
         "node": true,
         "mocha": true
      },
      "rules": {
         "no-console": "off",
         "quotes": [
            "warn",
            "single"
         ],
         "semi": [
            "warn",
            "always"
         ]
      }
   },
   "scripts": {
      "lint": "eslint gulpfile.js spec-*.js fetch-json.js",
      "pretest": "npm run lint && gulp build",
      "test": "mocha --timeout 10000 spec-*.js",
      "quick": "npm run lint && mocha --bail spec-node.js"
   },
   "dependencies": {},
   "devDependencies": {
      "@babel/core": "~7.1",
      "@babel/preset-env": "~7.1",
      "ajv": "~6.5",
      "eslint": "~5.6",
      "gulp": "~4.0",
      "gulp-babel": "~8.0",
      "gulp-babel-minify": "~0.5",
      "gulp-header": "~2.0",
      "gulp-rename": "~1.4",
      "gulp-replace": "~1.0",
      "gulp-size": "~3.0",
      "jsdom": "~12.1",
      "merge-stream": "~1.0",
      "mocha": "~5.2",
      "node-fetch": "~2.2",
      "promise-polyfill": "~8.1",
      "whatwg-fetch": "~3.0"
   }
}
